#include <iostream>
#include <vector>
using namespace std;
class Solution
{
public: 
	int removeDuplicates(vector<int>& nums)
	{
		int j = 0;
		for (int i = 1; i < nums.size(); i++)
		{
			if (nums.at(i) != nums.at(j))
			{
				j++;
				nums.at(j) = nums.at(i);
			}
		}
		return j+1;
	}
};
int main()
{
	vector<int> nums;
	nums.push_back(0);
	nums.push_back(0);
	nums.push_back(1);
	nums.push_back(1);
	nums.push_back(1);
	nums.push_back(2);
	nums.push_back(2);
	nums.push_back(3);
	nums.push_back(3);
	nums.push_back(4);
	Solution a;
	int k = a.removeDuplicates(nums);
	return 0;
}
